{
  "name": "my-todo-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "src",
    "client"
  ],
  "scripts": {
    "start": "concurrently \"cd src && npm run start:dev\" \"cd client && npm start\"",
    "build": "concurrently \"cd src && npm run build\" \"cd client && npm run build\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "prettier": "^3.6.2"
  },
  "overrides": {
    "nth-check": "2.1.1",
    "postcss": ">=8.4.31",
    "tmp": "^0.2.4",
    "svgo": "^4.0.0",
    "css-select": "^6.0.0",
    "resolve-url-loader": ">=5.0.0",
    "webpack-dev-server": "^5.2.2"
  }
}
