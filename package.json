{
  "name": "my-todo-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "src",
    "client"
  ],
  "scripts": {
    "start": ". ~/.nvm/nvm.sh && nvm use && concurrently \"npm:start:server\" \"npm:start:client\"",
    "start:server": "cd src && npm run start:dev",
    "start:client": "cd client && npm start",
    "build": "concurrently \"cd src && npm run build\" \"cd client && npm run build\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "devDependencies": {
    "@types/marked": "^5.0.2",
    "@types/sanitize-html": "^2.16.0",
    "concurrently": "^9.2.0",
    "prettier": "^3.6.2"
  },
  "overrides": {
    "nth-check": "2.1.1",
    "postcss": ">=8.4.31",
    "tmp": "^0.2.4",
    "svgo": "^4.0.0",
    "css-select": "^6.0.0",
    "resolve-url-loader": ">=5.0.0",
    "webpack-dev-server": "^4.15.2"
  },
  "dependencies": {
    "@langchain/google-genai": "^0.2.16",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/config": "^4.0.2",
    "date-fns": "^4.1.0",
    "marked": "^16.2.0",
    "react-datepicker": "^8.7.0",
    "sanitize-html": "^2.17.0",
    "zustand": "^5.0.8"
  }
}
